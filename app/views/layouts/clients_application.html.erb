<!DOCTYPE html>
<html>
<head>
  <title>VueJS</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application', media: 'all' %>
</head>

<body>
  <%= yield %>
  <% if client_signed_in? %>
    <%= stylesheet_pack_tag 'clients_application', media: 'all' %>
    <%= javascript_pack_tag 'clients_application' %>
  <% end %>
</body>
</html>
